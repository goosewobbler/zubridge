{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": ["tsconfig.json"],
  "tasks": {
    "//#format:check": {
      "inputs": ["**/*.{yml,json,md,ts,tsx,js}"]
    },
    "//#ci": {},
    "build": {
      "dependsOn": ["clean:output"],
      "cache": false
    },
    "build:packages": {
      "dependsOn": [
        "@zubridge/types#build",
        "@zubridge/core#build",
        "@zubridge/electron#build",
        "@zubridge/tauri#build",
        "@zubridge/ui#build",
        "@zubridge/apps-shared#build"
      ],
      "cache": false
    },
    "build:electron-examples": {
      "dependsOn": [
        "@zubridge/electron#build",
        "@zubridge/ui#build",
        "zubridge-electron-example#build:basic",
        "zubridge-electron-example#build:handlers",
        "zubridge-electron-example#build:reducers",
        "zubridge-electron-example#build:redux",
        "zubridge-electron-example#build:custom"
      ],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "clean:cache": {
      "cache": false
    },
    "clean:output": {
      "cache": false
    },
    "release": {
      "dependsOn": ["build:packages", "@zubridge/electron#test:unit", "@zubridge/tauri#test:unit"]
    },
    "test": {
      "dependsOn": ["test:unit", "test:e2e"]
    },
    "test:unit": {
      "dependsOn": ["build:packages"],
      "cache": false
    },
    "test:e2e": {
      "dependsOn": [
        "build:electron-examples",
        "test:e2e:electron-basic",
        "test:e2e:electron-reducers",
        "test:e2e:electron-handlers",
        "test:e2e:electron-redux",
        "test:e2e:electron-custom",
        "test:e2e:tauri-basic",
        "test:e2e:tauri-v1-basic"
      ],
      "cache": false
    },
    "test:e2e:electron-reducers": {
      "dependsOn": ["zubridge-electron-example#build:reducers"],
      "cache": false
    },
    "test:e2e:electron-handlers": {
      "dependsOn": ["zubridge-electron-example#build:handlers"],
      "cache": false
    },
    "test:e2e:electron-basic": {
      "dependsOn": ["zubridge-electron-example#build:basic"],
      "cache": false
    },
    "test:e2e:electron-redux": {
      "dependsOn": ["zubridge-electron-example#build:redux"],
      "cache": false
    },
    "test:e2e:electron-custom": {
      "dependsOn": ["zubridge-electron-example#build:custom"],
      "cache": false
    },
    "test:e2e:tauri-basic": {
      "dependsOn": ["zubridge-tauri-example#build"],
      "cache": false
    },
    "test:e2e:tauri-v1-basic": {
      "dependsOn": ["zubridge-tauri-v1-example#build"],
      "cache": false
    },
    "@zubridge/middleware#build": {
      "cache": false
    },
    "@zubridge/types#build": {
      "outputs": ["dist/**"],
      "cache": false
    },
    "@zubridge/electron#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "@zubridge/tauri#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "@zubridge/apps-shared#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "@zubridge/ui#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "@zubridge/electron#test:unit": {
      "dependsOn": ["@zubridge/electron#build"],
      "cache": false
    },
    "@zubridge/tauri#test:unit": {
      "dependsOn": ["@zubridge/tauri#build"],
      "cache": false
    },
    "zubridge-electron-example#build": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "zubridge-electron-example#build:reducers": {
      "dependsOn": ["^build"],
      "inputs": [
        "packages/electron/dist/**",
        "packages/ui/dist/**",
        "packages/apps-shared/dist/**",
        "packages/middleware/dist/**"
      ],
      "cache": false
    },
    "zubridge-electron-example#build:handlers": {
      "dependsOn": ["^build"],
      "inputs": [
        "packages/electron/dist/**",
        "packages/ui/dist/**",
        "packages/apps-shared/dist/**",
        "packages/middleware/dist/**"
      ],
      "cache": false
    },
    "zubridge-electron-example#build:basic": {
      "dependsOn": ["^build"],
      "inputs": [
        "packages/electron/dist/**",
        "packages/ui/dist/**",
        "packages/apps-shared/dist/**",
        "packages/middleware/dist/**"
      ],
      "cache": false
    },
    "zubridge-electron-example#build:redux": {
      "dependsOn": ["^build"],
      "inputs": [
        "packages/electron/dist/**",
        "packages/ui/dist/**",
        "packages/apps-shared/dist/**",
        "packages/middleware/dist/**"
      ],
      "cache": false
    },
    "zubridge-electron-example#build:custom": {
      "dependsOn": ["^build"],
      "inputs": [
        "packages/electron/dist/**",
        "packages/ui/dist/**",
        "packages/apps-shared/dist/**",
        "packages/middleware/dist/**"
      ],
      "cache": false
    },
    "zubridge-tauri-example#build": {
      "dependsOn": ["^build"],
      "inputs": [
        "packages/electron/dist/**",
        "packages/ui/dist/**",
        "packages/apps-shared/dist/**",
        "packages/middleware/dist/**"
      ],
      "outputs": ["apps/tauri-example/dist/**", "apps/tauri-example/src-tauri/target/**"],
      "cache": false
    },
    "zubridge-tauri-v1-example#build": {
      "dependsOn": ["^build"],
      "inputs": [
        "packages/electron/dist/**",
        "packages/ui/dist/**",
        "packages/apps-shared/dist/**",
        "packages/middleware/dist/**"
      ],
      "outputs": ["apps/tauri-v1-example/dist/**", "apps/tauri-v1-example/src-tauri/target/**"],
      "cache": false
    },
    "@zubridge/core#build": {
      "dependsOn": ["@zubridge/types#build"],
      "outputs": ["dist/**"],
      "cache": false
    }
  }
}
