{
  "name": "@zubridge/apps-shared",
  "version": "0.1.0",
  "private": true,
  "description": "Shared utilities and logic for Zubridge example apps",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "pnpm run clean:output && pnpm run clean:cache && shx rm -rf node_modules",
    "clean:output": "shx rm -rf dist",
    "clean:cache": "shx rm -rf .turbo",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@zubridge/core": "workspace:*",
    "@zubridge/types": "workspace:*"
  },
  "peerDependencies": {
    "zustand": "^4.3.0"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
