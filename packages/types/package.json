{
  "name": "@zubridge/types",
  "version": "2.0.1",
  "author": "Sam Maister <goosewobbler@protonmail.com>",
  "description": "Shared type definitions for Zubridge packages",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./internal": {
      "import": {
        "types": "./dist/internal.d.ts",
        "default": "./dist/internal.js"
      },
      "require": {
        "types": "./dist/internal.d.cts",
        "default": "./dist/internal.cjs"
      }
    },
    "./app": {
      "import": {
        "types": "./dist/app.d.ts",
        "default": "./dist/app.js"
      },
      "require": {
        "types": "./dist/app.d.cts",
        "default": "./dist/app.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "prebuild": "shx rm -rf dist && tsx ../../scripts/clean-cache.ts || echo 'Cache clean skipped'",
    "clean": "pnpm run clean:output && pnpm run clean:cache && shx rm -rf node_modules",
    "clean:output": "shx rm -rf dist",
    "clean:cache": "tsx ../../scripts/clean-cache.ts",
    "test": "vitest run",
    "package": "pnpm pack"
  },
  "peerDependencies": {
    "electron": "35.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "shx": "^0.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "zustand": "^5.0.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goosewobbler/zubridge.git"
  },
  "keywords": [
    "zustand",
    "state",
    "management",
    "types"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/goosewobbler/zubridge/issues"
  },
  "homepage": "https://github.com/goosewobbler/zubridge/tree/main/packages/types#readme"
}
