{
  "versionPrefix": "v",
  "preset": "angular",
  "baseBranch": "main",
  "sync": false,
  "commitMessage": "chore: release ${packageName}@${version} [skip-ci]",
  "tagTemplate": "${packageName}@${prefix}${version}",
  "skip": ["@zubridge/apps-shared", "e2e-*", "minimal-*", "zubridge-e2e"],
  "prereleaseIdentifier": "next"
}
