{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "productName": "zubridge-tauri-example-reducers",
  "version": "0.1.0",
  "identifier": "com.zubridge-tauri.example-reducers",
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  },
  "app": {
    "security": {
      "capabilities": ["main-capability"],
      "csp": {
        "default-src": ["'self'", "plugin:"],
        "img-src": ["'self'", "data:", "asset:", "https://asset.localhost"],
        "connect-src": [
          "'self'",
          "plugin:",
          "ipc://*",
          "tauri://*",
          "asset:",
          "https://asset.localhost",
          "ipc://localhost",
          "plugin:event|listen",
          "plugin:*"
        ],
        "script-src": ["'self'", "'unsafe-inline'", "plugin:"],
        "style-src": ["'self'", "'unsafe-inline'"]
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "zubridge-tauri-example-reducers",
        "width": 256,
        "height": 256,
        "label": "main"
      }
    ]
  }
}
