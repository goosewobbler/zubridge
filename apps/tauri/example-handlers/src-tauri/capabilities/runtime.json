{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "runtime-capability",
  "description": "Capability for runtime windows",
  "windows": ["runtime-*"],
  "permissions": [
    "core:default",
    "core:app:default",
    "core:window:default",
    "core:event:default",
    "core:webview:default",
    "core:webview:allow-create-webview-window",
    "core:window:allow-get-all-windows",
    "core:window:allow-create",
    "core:window:allow-close"
  ],
  "commands": ["get_state", "set_state", "update_state", "create_window", "close_window"]
}
