{
  "name": "minimal-zustand-basic",
  "version": "0.0.0",
  "description": "Minimal Electron app demonstrating Zubridge with Zustand basic mode",
  "private": true,
  "type": "module",
  "main": "out/main/index.js",
  "scripts": {
    "clean": "pnpm clean:cache && pnpm dlx shx rm -rf ./node_modules pnpm-lock.yaml",
    "clean:cache": "pnpm dlx shx rm -rf ./dist* ./out* .cache",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "dist": "pnpm build && electron-builder",
    "dist:dir": "pnpm build && electron-builder --dir"
  },
  "dependencies": {
    "@zubridge/core": "workspace:*",
    "@zubridge/electron": "workspace:*",
    "@zubridge/ui": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.0",
    "@zubridge/types": "workspace:*",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "electron": "35.0.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.0.0",
    "postcss": "^8.5.5",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.7.3",
    "vite": "^6.2.6"
  }
}
