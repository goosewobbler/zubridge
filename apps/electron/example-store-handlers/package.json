{
  "name": "zubridge-electron-example-store-handlers",
  "version": "0.0.1",
  "description": "An application demonstrating the use of zubridge-electron with action handlers which are located in the Zustand store",
  "main": "./out/main/index.js",
  "type": "module",
  "author": "goosewobbler",
  "homepage": "https://github.com/goosewobbler/zubridge-electron",
  "scripts": {
    "clean": "pnpm clean:output && pnpm dlx shx rm -rf ./node_modules pnpm-lock.yaml",
    "clean:output": "pnpm dlx shx rm -rf ./dist ./out",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --inspect --sourcemap",
    "build": "pnpm run typecheck && electron-vite build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zubridge-electron": "workspace:*",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.9",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.2",
    "electron": "^34.1.1",
    "electron-builder": "^25.1.8",
    "electron-vite": "^2.3.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "wdio-electron-service": "^7.3.1"
  }
}
