{
  "name": "minimal-zustand-reducers",
  "version": "0.0.0",
  "description": "Minimal Electron app demonstrating Zubridge with Zustand reducers mode",
  "private": true,
  "type": "module",
  "main": "out/main/index.js",
  "scripts": {
    "clean": "pnpm clean:cache && pnpm dlx shx rm -rf ./node_modules pnpm-lock.yaml",
    "clean:cache": "pnpm dlx shx rm -rf ./dist* ./out* .cache",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "dist": "pnpm build && electron-builder",
    "dist:dir": "pnpm build && electron-builder --dir",
    "test": "cross-env DEBUG=wdio-electron-service,webdriverio xvfb-maybe wdio run ./test/wdio.conf.ts",
    "test:package": "cross-env DEBUG=wdio-electron-service,webdriverio xvfb-maybe wdio run ./test/wdio.conf.ts",
    "clean:logs": "pnpx shx rm -rf ./wdio-logs-*"
  },
  "dependencies": {
    "@zubridge/electron": "next",
    "clsx": "^2.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "scheduler": "^0.26.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react": "^4.4.0",
    "@wdio/cli": "9.19.2",
    "@wdio/globals": "9.17.0",
    "@wdio/local-runner": "9.19.2",
    "@wdio/mocha-framework": "9.19.2",
    "@wdio/spec-reporter": "9.19.2",
    "@zubridge/types": "next",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "electron": "37.3.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "is-ci": "^4.1.0",
    "postcss": "^8.5.5",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "wdio-electron-service": "^9.1.1",
    "xvfb-maybe": "^0.2.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "esbuild"
    ]
  }
}
